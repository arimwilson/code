pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
-- tetris in pico-8

-- variables
local board = {}
local current_piece
local next_piece
local piece_x
local piece_y
local game_speed = 0.5
local time = 0
local score = 0
local pieces_played = 0
local pieces = {
    {
        { 1, 1, 1 },
        { 0, 1, 0 },
        color = 8
    },
    {
        { 1, 1 },
        { 1, 1 },
        color = 9
    },
    {
        { 1, 1, 0 },
        { 0, 1, 1 },
        color = 10
    },
    {
        { 0, 1, 1 },
        { 1, 1, 0 },
        color = 11
    },
    {
        { 1, 1, 1, 1 },
        color = 12
    },
    {
        { 1, 1, 1 },
        { 1, 0, 0 },
        color = 13
    },
    {
        { 1, 1, 1 },
        { 0, 0, 1 },
        color = 14
    }
}
cartdata("tetris_highscore")
local highscore = dget(0) or 0

-- initialize and turn on music
function init_board()
    music(track_listing[flr(rnd(#track_listing)) + 1].start, 120) 
    board = {}
    for y = 1, 20 do
        board[y] = {}
        for x = 1, 10 do
            board[y][x] = 0
        end
    end
end

-- rotate
function rotate(piece, direction)
    direction = direction or 1 -- default to clockwise rotation if direction is not provided
    local new_piece = {}
    if direction == 1 then
        local size = #piece
        local new_size = #piece[1]

        for x = 1, new_size do
            new_piece[x] = {}
            for y = 1, size do
                new_piece[x][y] = piece[size - y + 1][x]
            end
        end
    else
        new_piece = rotate(piece, 1)
        new_piece = rotate(new_piece, 1)
        new_piece = rotate(new_piece, 1)
    end

    -- copy the color from the original piece to the rotated piece
    new_piece.color = piece.color

    return new_piece
end

function hard_drop(piece, px, py)
    while not collides(piece, px, py + 1) do
        py = py + 1
    end
    return py
end

-- collision
function collides(piece, px, py)
    for y = 1, #piece do
        for x = 1, #piece[y] do
            if piece[y][x] == 1 then
                local board_x, board_y = px + x, py + y
                if board_x < 1 or board_x > 10 or board_y > 20 or (board_y > 0 and board[board_y][board_x] ~= 0) then
                    return true
                end
            end
        end
    end
    return false
end

-- merge
function merge_piece(piece, px, py)
    for y = 1, #piece do
        for x = 1, #piece[y] do
            if piece[y][x] == 1 then
                board[py + y][px + x] = piece.color
            end
        end
    end
end

-- clear lines
function clear_lines()
    local lines_cleared = 0
    local y = 20
    while y > 0 do
        local filled = true
        for x = 1, 10 do
            if board[y][x] == 0 then
                filled = false
                break
            end
        end
        if filled then
            for remove_y = y, 2, -1 do
                board[remove_y] = board[remove_y - 1]
            end
            for x = 1, 10 do
                board[1][x] = 0
            end
            lines_cleared = lines_cleared + 1
        else
            y = y - 1
        end
    end

    score = score + lines_cleared * 100
    return lines_cleared
end

-- spawn
function spawn_piece()
    current_piece = next_piece or pieces[flr(rnd(#pieces)) + 1]
    next_piece = pieces[flr(rnd(#pieces)) + 1]
    piece_x = 4
    piece_y = 1

    pieces_played = pieces_played + 1
    score = score + 5

    if collides(current_piece, piece_x, piece_y) then
        init_board()
        score = 0
        pieces_played = 0
    end
end

-- draw
function draw_board()
    for y = 1, 20 do
        for x = 1, 10 do
            if board[y][x] ~= 0 then
                rectfill(x * 6 - 6, y * 6 - 6, x * 6 - 1, y * 6 - 1, board[y][x])
            end
        end
    end
end

function draw_piece(piece, px, py)
    for y = 1, #piece do
        for x = 1, #piece[y] do
            if piece[y][x] == 1 then
                rectfill((px + x) * 6 - 6, (py + y) * 6 - 6, (px + x) * 6 - 1, (py + y) * 6 - 1, piece.color)
            end
        end
    end
end

function draw_ghost_piece(piece, px, py)
    local ghost_y = hard_drop(piece, px, py)

    for y = 1, #piece do
        for x = 1, #piece[y] do
            if piece[y][x] == 1 then
                local posx = (px + x) * 6
                local posy = (ghost_y + y) * 6

                rect(posx - 6, posy - 6, posx - 1, posy - 1, 5) -- white outline
            end
        end
    end
end

function draw_next_piece()
    print("next:", 75, 15, 7)
    draw_piece(next_piece, 13, 4)
end

--track metadata
--tracks display in this order
track_listing = {
	{trackname="arpument", 
		start=0,
		},
	{trackname="walkabout", 
		start=11,
		},
	{trackname="factory fresh", 
		start=22,
		},
	{trackname="seaside town", 
		start=16,
		},
	{trackname="empire", 
		start=27,
		},
	{trackname="melancholy", 
		start=33,
		},
	{trackname="mission", 
		start=37,
		},
	{trackname="hijinx", 
		start=43,
		},
	{trackname="out of control", 
		start=53,
		},
	}

-- update
function _update()
    time = time + 1 / 30
    if time >= game_speed then
        time = time - game_speed
        if not collides(current_piece, piece_x, piece_y + 1) then
            piece_y = piece_y + 1
        else
            merge_piece(current_piece, piece_x, piece_y)
            clear_lines()
            spawn_piece()
            if score > highscore then
                highscore = score
                dset(0, highscore)
            end
        end
    end
    if btnp(0) and not collides(current_piece, piece_x - 1, piece_y) then
        piece_x = piece_x - 1
    end
    if btnp(1) and not collides(current_piece, piece_x + 1, piece_y) then
        piece_x = piece_x + 1
    end
    if btnp(2) and not collides(current_piece, piece_x, piece_y + 1) then
        piece_y = piece_y + 1
    end
    if btnp(3) then
        piece_y = hard_drop(current_piece, piece_x, piece_y)
    end
    if btnp(4) then
        local rotated_piece = rotate(current_piece, -1)
        if not collides(rotated_piece, piece_x, piece_y) then
            current_piece = rotated_piece
        end
    end
    if btnp(5) then
        local rotated_piece = rotate(current_piece, 1)
        if not collides(rotated_piece, piece_x, piece_y) then
            current_piece = rotated_piece
        end
    end
end

-- main loop
function _draw()
    cls()
    draw_board()
    draw_piece(current_piece, piece_x, piece_y)
    draw_ghost_piece(current_piece, piece_x, piece_y)
    draw_next_piece()

    -- draw a thin white outline around the game board
    rect(0, 0, 10 * 6, 20 * 6, 7)

    -- print the score and high score
    print("score: " .. score, 63, 1, 7)
    print("high score: " .. highscore, 63, 8, 7)
end

init_board()
spawn_piece()

